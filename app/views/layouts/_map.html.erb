<div class="col-md-12">
  <div style='width: 760px;'>
    <div id="map" style='width: 760px; height: 400px;'></div>
  </div>
</div>

<script>
  var myLatlng = new google.maps.LatLng(<%= @map_central_cooordinates.lat %>, <%= @map_central_cooordinates.lng %>);
  var mapOptions = {
    zoom: <%= @map_zoom %>,
    center: myLatlng
  }
  var map = new google.maps.Map(document.getElementById("map"), mapOptions);
  var marker = new google.maps.Marker(<%= raw @markers.to_json %>);
  marker.setMap(map);
</script>
